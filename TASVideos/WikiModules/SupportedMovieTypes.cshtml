@model SupportedMovieTypes
@{ 
	var entries = Model.Formats.Select(m => $"{m.Key}{(m.Value?.Deprecated == true ? " (not submittable) " : "")}");
}
<div>@string.Join(", ", entries)</div>
<div>
	<a Permission="DeprecateMovieParsers" asp-page="/Submissions/DeprecateMovieFormats" class="btn btn-primary btn-sm mt-2">Deprecate Movies</a>
</div>